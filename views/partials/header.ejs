          <input style="padding:3%" required="required" type="email" name="otpEmail" id="otpemail" title="Email address"
            placeholder="Enter your registered email" autocomplete="off" />
        </div>

        <!-- <div class="mobileno">
          <input required="required" type="tel" name="signupMobileNumber" id="signupMobileNumber"
            title="Enter your Mobile Number" placeholder="   Mobile Number (10 digits)*" maxlength="10" />
        </div> -->




        <label for="enterotp"><button style="margin-top:10%" class="continue" id="otpbutton" style="cursor: pointer">
            SEND OTP
          </button></label>


      </div>

    </form>
    <div style="margin-top: 50px"></div>
    <hr />
    <div class="loginsocial">Login with social media</div>
    <div class="social">
      <div class="facebook">
        <img
          src="https://icons-for-free.com/iconfiles/png/512/facebook+logo+logo+website+icon-1320190502625926346.png" />
      </div>
      <div class="google">
        <img
          src="data:image/png;base64," />
      </div>
    </div>

  </div>




</div>

<div class="center">
  <input type="checkbox" id="enterotp" />
  <div class="container" id="containerjs">

    <label for="enterotp" id="formclose" class="close-btn fas fa-times" title="close"></label>
    <h4>ENTER OTP</h4>
    <form id="otp">
      <div class="otp">
        <div class="email">
          <input style="padding:3%" required="required" type="email" name="otpEmail" id="otpemail" title="Email address"
