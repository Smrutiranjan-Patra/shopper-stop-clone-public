<%- include('../partials/metadata', {stylesheet: "../css/men.css", footer: "../css/footer.css", header:"../css/header.css"}); %>
<body onload = "senddata(`<%=JSON.stringify(product)%>`, `<%=type%>`, `<%=totalPages%>`,`<%=page%>`)">
  <%- include('../partials/header', {}); %>
  <div class="slideshow-container">
    <div style="color: gray; margin-bottom: 10px;">Home / Men / Clothing</div>
    <div class="mySlides fade" style="display: none;">
        <img width="100%"
            src="https://cdn.dynamicyield.com/api/8770829/images/981c39ba51b4__plp--adi-_-rbk---static.jpg">
    </div>
    <div class="mySlides fade" style="display: none;">
        <img width="100%"
            src="https://cdn.dynamicyield.com/api/8770829/images/38bce4010910b__Shoppers-Stop-Banners-Web-1232px216px.jpg">
    </div>
    <div class="mySlides fade" style="display: block;">
        <img width="100%"
            src="https://cdn.dynamicyield.com/api/8770829/images/1679f7f8c71c6__TSHIRT--web--01.jpg">
    </div>
</div>
<br>
<div style="text-align: center; margin-top: -50px; position: relative">
    <span class="dot" onclick="currentSlide(1)"></span>
    <span class="dot" onclick="currentSlide(2)"></span>
    <span class="dot active" onclick="currentSlide(3)"></span>
</div>
    <div id="all">
      <%- include('../partials/leftsort', {}); %>
    <div id="rightpart">
    <div id="topsortbar">
      <h2 id="category text" style="text-align: left;">Men</h2>
      <div id="topsorting">
        <div>
          <p>Sort by:</p>
        </div>
        <div><button id="relevence">Relevance</button>|</div>
        <div><button id="Popular">Popular</button>|</div>
        <div><button id="New">New</button>|</div>
        <div><button id="Discount">Discount</button>|</div>
        <div><button id="Best_Seller">Best Seller</button>|</div>
        <div>
          Price:<button id="sortlow">Low</button>|<button id="sorthigh">
            High
          </button>
        </div>
      </div>
      <hr />
      <div id="topbelowsort">
        <li id="Sort_GENDER">GENDER</li>
        <li id="Sort_FABRIC">FABRIC</li>
        <li id="Sort_LENGTH">LENGTH</li>
        <li id="Sort_OCCASION">OCCASION</li>
        <li id="Sort_CATEGORY">CATEGORY</li>
      </div>
      <div id="topbelowsortoptions">
        <div class="topbelowsortoptions_dis" id="gendersort">
          <input type="checkbox" id="check" /><label for="check">Boys</label>

          <input type="checkbox" id="check" /><label for="check">Mens</label>

          <input type="checkbox" id="check" /><label for="check">Unisex</label>

          <input type="checkbox" id="check" /><label for="check">Women</label>
        </div>
        <div class="topbelowsortoptions_dis" id="fabricsort">
          <input type="checkbox" id="check" /><label for="check"> Cotton</label>

          <input type="checkbox" id="check" /><label for="check"
            >Cotton Nylon</label
          >

          <input type="checkbox" id="check" /><label for="check"
            >Cotton Stretch</label
          >

          <input type="checkbox" id="check" /><label for="check"> Knit</label>
          <input type="checkbox" id="check" /><label for="check">
            Leather</label
          >
        </div>
        <div class="topbelowsortoptions_dis" id="lengthsort">
          <input type="checkbox" id="check" /><label for="check"
            >Crew lengths</label
          >

          <input type="checkbox" id="check" /><label for="check"
            >Full Length</label
          >

          <input type="checkbox" id="check" /><label for="check"
            >Hip Length</label
          >

          <input type="checkbox" id="check" /><label for="check"
            >Mid Calf</label
          >
        </div>
        <div class="topbelowsortoptions_dis" id="occasionsort">
          <input class="sortingbelow" type="checkbox" id="workwear" /><label
            for="workwear"
            >Work Wear</label
          >

          <input class="sortingbelow" type="checkbox" id="formal" /><label
            for="formal"
            >Formal</label
          >

          <input class="sortingbelow" type="checkbox" id="party" /><label
            for="party"
            >Party</label
          >

          <input class="sortingbelow" type="checkbox" id="casual" /><label
            for="casual"
            >Casual</label
          >
        </div>

        <div class="topbelowsortoptions_dis" id="categorysort">
          <input type="checkbox" id="check" /><label for="check">
            Accessories</label
          >

          <input type="checkbox" id="check" /><label for="check">Caps</label>

          <input type="checkbox" id="check" /><label for="check">Tie</label>

          <input type="checkbox" id="check" /><label for="check">Socks </label>
          <input type="checkbox" id="check" /><label for="check"
            >Handkerchief
          </label>
        </div>
      </div>
    </div>
    <hr>


    <div id="product_container"></div>
    <div id="paginationcontainer" class="pagination_section"></div>
  </div>
</div>
<div id="foo">
      <%- include('../partials/footer', {}); %>
</div>
</body>
</html>
<script src="../script/signin.js"></script>
<script src="../script/men.js"></script>